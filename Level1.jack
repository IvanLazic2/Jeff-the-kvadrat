		}
		let Collectables = 0;
		let i = 0;
		while (i < (MapWidth * MapHeight))
		{
			let Map[i] = 0;
			let i = i + 1;
		}
		do Map.dispose();
		let Map = 0;
		let MapWidth = 0;
		let MapHeight = 0;
		do Memory.deAlloc(this);
		return;
	}
