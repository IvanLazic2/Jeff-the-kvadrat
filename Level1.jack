			do collectable.dispose();
			let collectable = 0;
			let i = i + 1;
		}
		let CollectablesCount = 0;
		if (~(PlatformsCount = 0))
		{
			do Platforms.dispose();
		}
		let Platforms = 0;
		if (~(ObstaclesCount = 0))
		{
